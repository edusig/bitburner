{
  "compilerOptions": {
    "module": "ES6",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ESNext",
    "noImplicitAny": true,
    "sourceMap": false,
    "strict": false,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*", "src/types/*"],
      "lib-util-auto-nuke.js": ["src/lib/util/auto-nuke"],
      "lib-util-calc-threads.js": ["src/lib/util/calc-threads"],
      "lib-util-constants.js": ["src/lib/util/constants"],
      "lib-util-current-open-port-software.js": ["src/lib/util/current-open-port-software"],
      "lib-util-server-info.js": ["src/lib/util/server-info"]
    }
  },
  "include": ["src/**/*"]
}
